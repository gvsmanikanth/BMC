<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/bmc/settings/wcm/templates/form-event-thank-you"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="bmc/components/structure/page"
        bodyClass="page-campaign-landing bmc-nav-loader-nav">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <header_event_form
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bmc/components/content/header-event-form"
                    heading="Thanks."/>
                <maincontentcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bmc/components/structure/maincontentcontainer">
                    <_x0031_00contentcontain
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="bmc/components/structure/100contentcontainer">
                        <center
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/responsivegrid">
                            <title
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Title"
                                sling:resourceType="bmc/components/content/title"
                                className="black at-element-marker"
                                type="h2"/>
                            <htmlarea
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="bmc/components/content/htmlarea"
                                text="Enter HTML here"/>
                        </center>
                    </_x0031_00contentcontain>
                </maincontentcontainer>
            </responsivegrid>
        </root>
    </jcr:content>
</jcr:root>
