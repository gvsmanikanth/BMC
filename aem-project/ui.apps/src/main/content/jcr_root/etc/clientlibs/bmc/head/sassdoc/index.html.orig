	$options: map-merge((
		selector: false
	), $options);

	$selector: map-get($options, selector);

	position: relative;	// context for direct descendant

	// container to maintain aspect ratio
	&amp;::before {
		content: &#39;&#39;;
		display: block;
		padding-top: percentage(parseInt($height) / parseInt($width));
	}

	@if $selector == false {
		&amp; &gt; embed,
		&amp; &gt; iframe,
		&amp; &gt; object,
		&amp; &gt; video,
		&amp; &gt; a,
		&amp; &gt; div,
		&amp; &gt; img {
			height: 100% !important;
			left: 0;
			position: absolute !important;
			top: 0;
			width: 100% !important;
		}
	}
	@else if $selector != none {
		&amp; &gt; #{$selector} {
			height: 100% !important;
			left: 0;
			position: absolute !important;
			top: 0;
			width: 100% !important;
		}
	}
 }" data-collapsed="@mixin aspect-ratio($width, $height, $selector: false) { ... }"><code>@mixin aspect-ratio($width, $height, $selector: false) { ... }</code></pre><a href="https://github.com/connectivedx/Phoenix/tree/master/Assets/src/mixins\_aspect-ratio.scss#L9-L49" class="item__source-link" target="_blank">View source</a></div><h3 class="item__sub-heading">Description</h3><div class="item__description"><p>Creates the necessary dimensions for an element to have a fixed aspect ratio.</p></div><h3 class="item__sub-heading">Parameters</h3><table class="item__parameters"><thead><tr><th scope="col"><span class="visually-hidden">parameter</span>Name</th><th scope="col"><span class="visually-hidden">parameter</span>Description</th><th scope="col"><span class="visually-hidden">parameter</span>Type</th><th scope="col"><span class="visually-hidden">parameter</span>Default value</th></tr></thead><tbody><tr class="item__parameter"><th scope="row" data-label="name"><code>$width</code></th><td data-label="desc"><p>The desired width of the element&#39;s aspect ratio.</p></td><td data-label="type"><code>Pixels</code> or <code>Integer</code></td><td data-label="default">&mdash;<span class="visually-hidden">none</span></td></tr><tr class="item__parameter"><th scope="row" data-label="name"><code>$height</code></th><td data-label="desc"><p>The desired height of the element&#39;s aspect ratio.</p></td><td data-label="type"><code>Pixels</code> or <code>Integer</code></td><td data-label="default">&mdash;<span class="visually-hidden">none</span></td></tr><tr class="item__parameter"><th scope="row" data-label="name"><code>$selector</code></th><td data-label="desc"><p>The selector of the child.</p></td><td data-label="type"><code>String</code></td><td data-label="default"><code>false</code></td></tr></tbody></table><h3 class="item__sub-heading">Example</h3><div class="item__example example"><pre class="example__code language-scss"><code>aspect-ratio(4, 3)</code></pre></div><div class="item__example example"><pre class="example__code language-scss"><code>aspect-ratio(4, 3, (selector: iframe))</code></pre></div><h3 class="item__sub-heading">Requires</h3><ul class="list-unstyled"><li class="item__description item__description--inline"><span class="item__cross-type">[function]</span> <a href="#function-parseInt"><code>parseInt</code></a></li></ul></section><section class="main__item container item" id="mixin-gradient"><h3 class="item__heading"><a class="item__name" href="#mixin-gradient">gradient</a></h3><div class="item__code-wrapper"><pre class="item__code item__code--togglable language-scss" data-current-state="collapsed" data-expanded="@mixin gradient($direction, $colorStops) { 

	// add &quot;to&quot; prefix on valid $directions and set default fallback
	@if is-direction($direction) {
		$direction: to $direction;
	}
	@else {
		$colorStops: join($direction, $colorStops, comma); // ensure colors are passed if no direction is passed
		$direction: to bottom; // set default direction
	}

	//Styles
