<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:rep="internal"
          jcr:primaryType="sling:OrderedFolder"
          hidden="true">
    <login jcr:mixinTypes="[granite:AuthenticationRequired,rep:CugMixin]" jcr:primaryType="cq:Page">
        <jcr:content jcr:primaryType="cq:PageContent" jcr:title="login" sling:resourceType="auth-redirect"/>
        <rep:cugPolicy jcr:primaryType="rep:CugPolicy" rep:principalNames="[everyone]"/>
    </login>
    <logout jcr:primaryType="cq:Page">
        <jcr:content jcr:primaryType="cq:PageContent" jcr:title="logout" sling:resourceType="auth-redirect"/>
    </logout>
</jcr:root>
