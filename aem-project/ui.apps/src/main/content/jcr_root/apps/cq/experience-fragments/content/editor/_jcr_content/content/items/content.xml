<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:vlt="http://www.day.com/jcr/vault/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:mixinTypes="[vlt:FullCoverage]"
    jcr:primaryType="sling:OrderedFolder"
    sling:resourceType="cq/gui/components/authoring/editorpanel"
    default="{Boolean}true"
    id="Content">
    <header
        jcr:primaryType="nt:unstructured"
        class="editor-GlobalBar js-editor-PanelHeader">
        <items jcr:primaryType="nt:unstructured">
            <headerbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/headerbar"
                class="header-main">
                <items jcr:primaryType="nt:unstructured">
                    <togglesidepanel
                        granite:class="toggle-sidepanel editor-GlobalBar-item js-editor-SidePanel-toggle"
                        granite:title="Toggle Side Panel"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        disabled="{Boolean}true"
                        icon="railLeft"
                        iconSize="S"
                        variant="minimal"/>
                    <pageinfo
                        granite:class="editor-GlobalBar-item foundation-toggleable-control"
                        granite:id="pageinfo-trigger"
                        granite:title="Page Information"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                        align="left"
                        hideText="{Boolean}true"
                        href="#pageinfo-popover"
                        icon="properties"
                        iconSize="S"
                        text="Page Information"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-toggleable-control-target="#pageinfo-popover"/>
                    </pageinfo>
                    <pageinfopopover
                        granite:class="pageinfo"
                        granite:id="pageinfo-popover"
                        granite:title="Page Information"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/popover">
                        <items jcr:primaryType="nt:unstructured">
                            <list
                                granite:id="pageinfo-data"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/pageinfo/list">
                                <items jcr:primaryType="nt:unstructured">
                                    <status
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/pageinfo/status"/>
                                    <properties
                                        granite:title="Open Properties"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/pageinfo/properties"
                                        text="Open Properties">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/renderconditions/canmodify"
                                            path="${requestPathInfo.suffix}"/>
                                    </properties>
                                    <rollout
                                        granite:id="rolloutBtn"
                                        granite:title="Rollout Page"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Rollout Page">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="wcm/msm/components/touch-ui/renderconditions/isblueprint"
                                            path="${requestPathInfo.suffix}"/>
                                    </rollout>
                                    <workflow
                                        granite:class="cq-author-start-workflow start-workflow-activator"
                                        granite:title="Start Workflow"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Start Workflow">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            path="${requestPathInfo.suffix}"/>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/renderconditions/canwriteworkflow"/>
                                    </workflow>
                                    <lock
                                        granite:class="cq-author-lock-page"
                                        granite:title="Lock Page"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Lock Page">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                                <islocked
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/renderconditions/islocked"
                                                    path="${requestPathInfo.suffix}"/>
                                            </not>
                                            <canmodify
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/renderconditions/canmodify"
                                                path="${requestPathInfo.suffix}"/>
                                        </granite:rendercondition>
                                    </lock>
                                    <unlock
                                        granite:class="cq-author-unlock-page"
                                        granite:title="Unlock Page"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Unlock Page">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <islocked
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/renderconditions/islocked"
                                                path="${requestPathInfo.suffix}"/>
                                            <canmodify
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/renderconditions/canmodify"
                                                path="${requestPathInfo.suffix}"/>
                                        </granite:rendercondition>
                                    </unlock>
                                    <publish-social
                                        granite:class="cq-authoring-actions-quickpublish-activator"
                                        granite:title="Publish Experience Fragment"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                                        href="${&quot;/libs/cq/experience-fragments/content/experience-fragments/previewvariation.html&quot;}${requestPathInfo.suffix}"
                                        target="_blank"
                                        text="Social Post"/>
                                    <publish
                                        granite:class="cq-authoring-actions-quickpublish-activator"
                                        granite:title="Publish Page"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Publish Page">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            edit="{Boolean}true"
                                            later="{Boolean}false"
                                            path="${requestPathInfo.suffix}"
                                            references-url="${granite:url(&quot;/libs/wcm/core/content/reference.json&quot;)}"
                                            replication-url="${granite:url(&quot;/bin/replicate.json&quot;)}"
                                            url="${granite:url(&quot;/libs/wcm/core/content/sites/publishpagewizard.html&quot;)}"/>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <canreplicate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/renderconditions/canreplicate"
                                                path="${requestPathInfo.suffix}"/>
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                                <islocked
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/renderconditions/islocked"
                                                    path="${requestPathInfo.suffix}"/>
                                            </not>
                                        </granite:rendercondition>
                                    </publish>
                                    <requestpublish
                                        granite:class="cq-authoring-actions-quickpublish-activator"
                                        granite:title="Request Publication"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Request Publication">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            edit="{Boolean}true"
                                            later="{Boolean}true"
                                            path="${requestPathInfo.suffix}"
                                            references-url="${granite:url(&quot;/libs/wcm/core/content/reference.json&quot;)}"
                                            replication-url="${granite:url(&quot;/bin/replicate.json&quot;)}"
                                            url="${granite:url(&quot;/libs/wcm/core/content/sites/publishpagewizard.html&quot;)}"/>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                                <canreplicate
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/renderconditions/canreplicate"
                                                    path="${requestPathInfo.suffix}"/>
                                                <islaunchresource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/renderconditions/islaunchresource"
                                                    path="${requestPathInfo.suffix}"/>
                                            </not>
                                            <workflowconditions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/pageinfo/renderconditions/replicate/workflowconditions"
                                                model="activation"
                                                path="${requestPathInfo.suffix}"/>
                                        </granite:rendercondition>
                                    </requestpublish>
                                    <unpublish
                                        granite:class="foundation-toggleable-control pageinfo-unpublish-activator"
                                        granite:title="Unpublish Page"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Unpublish Page">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            foundation-toggleable-control-target=".pageinfo-unpublish-confirm"
                                            path="${requestPathInfo.suffix}"/>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <canreplicate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/renderconditions/canreplicate"
                                                path="${requestPathInfo.suffix}"/>
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                                <islocked
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/renderconditions/islocked"
                                                    path="${requestPathInfo.suffix}"/>
                                            </not>
                                        </granite:rendercondition>
                                    </unpublish>
                                    <requestunpublish
                                        granite:class="foundation-toggleable-control pageinfo-unpublish-activator"
                                        granite:title="Request Unpublication"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Request Unpublication">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            foundation-toggleable-control-target=".pageinfo-requestunpublish-confirm"
                                            path="${requestPathInfo.suffix}"/>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                                <canreplicate
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/renderconditions/canreplicate"
                                                    path="${requestPathInfo.suffix}"/>
                                                <islaunchresource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/renderconditions/islaunchresource"
                                                    path="${requestPathInfo.suffix}"/>
                                            </not>
                                            <workflowconditions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/pageinfo/renderconditions/replicate/workflowconditions"
                                                model="deactivation"
                                                path="${requestPathInfo.suffix}"/>
                                        </granite:rendercondition>
                                    </requestunpublish>
                                    <promotelaunch
                                        granite:title="Promote Launch"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/pageinfo/promotelaunch"
                                        text="Promote Launch">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/renderconditions/islaunchresource"
                                            path="${requestPathInfo.suffix}"/>
                                    </promotelaunch>
                                    <analytics
                                        granite:class="open-contentinsight"
                                        granite:title="Open Analytics &amp; Recommendations"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Open Analytics &amp; Recommendations">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/renderconditions/hasanalytics"
                                            path="${requestPathInfo.suffix}"/>
                                    </analytics>
                                    <viewaspublished
                                        granite:class="pageinfo-viewaspublished"
                                        granite:title="View as Published"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="View as Published"/>
                                    <viewinadmin
                                        granite:class="pageinfo-adminview"
                                        granite:title="View in Admin"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="View in Admin">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            adminurl="/sites.html"
                                            path="${requestPathInfo.suffix}"/>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                            <islaunchresource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/renderconditions/islaunchresource"
                                                path="${requestPathInfo.suffix}"/>
                                        </granite:rendercondition>
                                    </viewinadmin>
                                    <classicui
                                        granite:class="classicui-switcher"
                                        granite:hidden="{Boolean}true"
                                        granite:title="Open in Classic UI"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Open in Classic UI"/>
                                    <uploadzip
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="wcm/designimporter/components/touch-ui/uploadbutton"
                                        accept="application/zip,application/x-zip-compressed"
                                        buttonClass="endor-List-item endor-List-item--interactive coral-MinimalButton js-editor-PageInfo-closePopover editor-PageInfo-action"
                                        class="cq-designimporter-actions-upload"
                                        text="Upload Zip">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <isimporterpage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="wcm/designimporter/components/touch-ui/renderconditions/isimporterpage"
                                                path="${requestPathInfo.suffix}"/>
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                                <hascanvas
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="wcm/designimporter/components/touch-ui/renderconditions/hascanvas"
                                                    path="${requestPathInfo.suffix}"/>
                                            </not>
                                        </granite:rendercondition>
                                    </uploadzip>
                                    <cleardesign
                                        granite:class="cq-designimporter-actions-clear"
                                        granite:title="Clear Design"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Clear Design">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <isimporterpage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="wcm/designimporter/components/touch-ui/renderconditions/isimporterpage"
                                                path="${requestPathInfo.suffix}"/>
                                            <hascanvas
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="wcm/designimporter/components/touch-ui/renderconditions/hascanvas"
                                                path="${requestPathInfo.suffix}"/>
                                        </granite:rendercondition>
                                    </cleardesign>
                                    <downloaddesign
                                        granite:class="cq-designimporter-actions-download"
                                        granite:title="Download Imported Zip"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Download Imported Zip">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <isimporterpage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="wcm/designimporter/components/touch-ui/renderconditions/isimporterpage"
                                                path="${requestPathInfo.suffix}"/>
                                            <hascanvas
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="wcm/designimporter/components/touch-ui/renderconditions/hascanvas"
                                                path="${requestPathInfo.suffix}"/>
                                        </granite:rendercondition>
                                    </downloaddesign>
                                    <help
                                        granite:class="editor-shell-onboarding-trigger"
                                        granite:title="Help"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/button"
                                        text="Help"/>
                                </items>
                            </list>
                        </items>
                    </pageinfopopover>
                    <contexthub
                        granite:class="editor-GlobalBar-item js-editor-ContextHubTrigger"
                        granite:title="ContextHub"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        disabled="{Boolean}true"
                        icon="demographic"
                        iconSize="S"
                        variant="minimal"/>
                    <lockmode
                        granite:class="cq-author-unlock-page editor-GlobalBar-item"
                        granite:id="unlock-page-trigger"
                        granite:title="Locked page"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        disabled="{Boolean}true"
                        hideText="{Boolean}true"
                        icon="lockOn"
                        iconSize="S"
                        variant="minimal"/>
                    <undo
                        granite:class="cq-History-control editor-GlobalBar-item hide"
                        granite:title="Undo"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        hideText="{Boolean}true"
                        icon="undo"
                        text="Undo"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            history-control="undo"/>
                    </undo>
                    <redo
                        granite:class="cq-History-control editor-GlobalBar-item hide"
                        granite:title="Redo"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        hideText="{Boolean}true"
                        icon="redo"
                        text="Redo"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            history-control="redo"/>
                    </redo>
                    <emulator
                        granite:class="editor-GlobalBar-item js-editor-EmulatorBar-toggle"
                        granite:title="Emulator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        disabled="{Boolean}true"
                        icon="devices"
                        iconSize="S"
                        variant="minimal"/>
                    <annotationbadge
                        granite:title="Annotate"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/annotationsbadge"
                        align="right"/>
                    <preview
                        granite:class="editor-GlobalBar-item js-editor-LayerSwitcherTrigger editor-GlobalBar-previewTrigger js-editor-GlobalBar-previewTrigger"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="right"
                        text="Preview"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            layer="Preview"/>
                    </preview>
                    <selectlayer
                        granite:class="editor-GlobalBar-item foundation-toggleable-control editor-GlobalBar-layerSwitcher"
                        granite:title="Select another mode"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                        align="right"
                        href="#selectlayer-popover"
                        icon="chevronDown"
                        iconSize="XS"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-toggleable-control-target="#selectlayer-popover"/>
                    </selectlayer>
                    <currentlayer
                        granite:class="editor-GlobalBar-item js-editor-GlobalBar-layerCurrent editor-GlobalBar-layerCurrent js-editor-LayerSwitcherTrigger"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="right"
                        text="Edit"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            layer="Edit"/>
                    </currentlayer>
                    <selectlayerpopover
                        granite:id="selectlayer-popover"
                        granite:title="Select another mode"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/popover">
                        <items jcr:primaryType="nt:unstructured">
                            <buttonlist
                                granite:class="editor-GlobalBar-popoverContent editor-GlobalBar-layerSwitcherPopoverContent"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"/>
                        </items>
                    </selectlayerpopover>
                </items>
            </headerbar>
            <statusbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/statusbar"
                statusType="editor"/>
            <emulatorbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/emulatorbar">
                <items jcr:primaryType="nt:unstructured">
                    <opendevicepopover
                        granite:class="foundation-toggleable-control"
                        granite:title="Select device"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        hideText="{Boolean}true"
                        icon="deviceDesktop"
                        iconSize="S"
                        text="Select device"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            foundation-toggleable-control-target="#js-editor-EmulatorDevicePopover"/>
                    </opendevicepopover>
                    <rotate
                        granite:class="js-EmulatorDeviceRotate editor-EmulatorDeviceRotate"
                        granite:title="Rotate device"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        hideText="{Boolean}true"
                        icon="deviceRotatePortrait"
                        iconSize="S"
                        text="Rotate device"
                        variant="minimal">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            device="native"/>
                    </rotate>
                </items>
            </emulatorbar>
            <devicepopover
                granite:id="js-editor-EmulatorDevicePopover"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/popover">
                <items jcr:primaryType="nt:unstructured">
                    <list
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/emulatordevicelist"/>
                </items>
            </devicepopover>
        </items>
    </header>
    <content
        jcr:primaryType="nt:unstructured"
        id="ContentScrollView">
        <items jcr:primaryType="nt:unstructured">
            <contentwrapper
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/contentwrapper"/>
            <overlaywrapper
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/overlaywrapper"/>
            <dnddefaultghost
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dnddefaultghost"/>
            <startworkflowmodal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/workflow/startworkflow"/>
            <confirmunpublish
                granite:class="foundation-toggleable pageinfo-unpublish-confirm"
                jcr:primaryType="nt:unstructured"
