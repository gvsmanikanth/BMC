<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="RelatedCTAsList"
          sling:resourceSuperType="/apps/bmc/components/content/related-CTAs/related-CTAs-landing/cq:dialog">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <columns jcr:primaryType="nt:unstructured" jcr:title="RelatedCTAsList">
                <items jcr:primaryType="nt:unstructured">
                    <column jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <singularHeadingText
                                    jcr:primaryType="nt:unstructured"
                                    sling:orderBefore="linkItems"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldDescription="Enter “Featured offering” in sentence case if there is only one item. Do not include the : as it will be automatically appended by the template."
                                    fieldLabel="Singular Heading"
                                    name="./singularHeadingText" />
                            <pluralHeadingText
                                    jcr:primaryType="nt:unstructured"
                                    sling:orderBefore="linkItems"
                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                    fieldDescription="Enter “Featured offerings” in sentence case if there is only one item. Do not include the : as it will be automatically appended by the template."
                                    fieldLabel="Plural Heading"
                                    name="./pluralHeadingText" />
                        </items>
                    </column>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>