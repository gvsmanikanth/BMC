<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Basic"
    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
    <items jcr:primaryType="nt:unstructured">
        <column
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
                <title
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Title and Tags"
                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                    <items jcr:primaryType="nt:unstructured">
                        <showbreadcrumbs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            name="./showBreadcrumbs"
                            renderReadOnly="{Boolean}false"
                            text="Show Breadcrumbs"
                            value="true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="showBreadcrumbs"/>
                        </showbreadcrumbs>
                    </items>
                </title>
                <moretitles
                    cq:showOnCreate="{Boolean}true"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="More Titles and Description"
                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                    <items jcr:primaryType="nt:unstructured">
                        <browserTitle
                            jcr:primaryType="nt:unstructured"
                            sling:orderBefore="pagetitle"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Browser Title"
                            name="./browserTitle"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="browserTitle"/>
                        </browserTitle>
                        <pagetitle
                            cq:showOnCreate="{Boolean}true"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Page Title"
                            name="./pageTitle"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="pageTitle"/>
                        </pagetitle>
                        <navigationtitle
                            cq:showOnCreate="{Boolean}true"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            allowBulkEdit="{Boolean}true"
                            fieldLabel="Navigation Title"
                            name="./navTitle"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="navTitle"/>
                        </navigationtitle>
                        <docTypePrefix
                            granite:class="cq-dialog-dropdown-showhide"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                            fieldLabel="Select document asset prefix"
                            name="./docTypePrefix">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                cq-dialog-dropdown-showhide-target=".doc-type-prefix-showhide-target"/>
                            <items jcr:primaryType="nt:unstructured">
                                <noneselected
                                    jcr:primaryType="nt:unstructured"
                                    text="None selected"
                                    value=""/>
                                <Analyst_x0020_Research
                                    jcr:primaryType="nt:unstructured"
                                    text="Analyst research"
                                    value="Analyst research"/>
                                <Article_x0020_Blog
                                    jcr:primaryType="nt:unstructured"
                                    text="Article/Blog"
                                    value="Article/Blog"/>
                                <Competitive_x0020_Comparison
                                    jcr:primaryType="nt:unstructured"
                                    text="Competitive Comparison"
                                    value="Competitive Comparison"/>
                                <custom
                                    granite:hide="${cqDesign.disableChildren}"
                                    jcr:primaryType="nt:unstructured"
                                    text="Custom"
                                    value="custom"/>
                                <Customer_x0020_Story
                                    jcr:primaryType="nt:unstructured"
                                    text="Customer story"
                                    value="Customer story"/>
                                <DataSheet
                                    jcr:primaryType="nt:unstructured"
                                    text="Datasheet"
                                    value="Datasheet"/>
                                <Demo
                                    jcr:primaryType="nt:unstructured"
                                    text="Demo"
                                    value="Demo"/>
                                <eBook
                                    jcr:primaryType="nt:unstructured"
                                    text="E-book"
                                    value="E-book"/>
                                <Event
                                    jcr:primaryType="nt:unstructured"
                                    text="Event"
                                    value="Event"/>
                                <Infographic
                                    jcr:primaryType="nt:unstructured"
                                    text="Infographic"
                                    value="Infographic"/>
                                <Interactive_x0020_Tool
                                    jcr:primaryType="nt:unstructured"
                                    text="Interactive Tool"
                                    value="Interactive Tool"/>
                                <Tech_x0020_Note
                                    jcr:primaryType="nt:unstructured"
                                    text="Tech Note"
                                    value="Tech Note"/>
                                <Trial
                                    jcr:primaryType="nt:unstructured"
                                    text="Trial"
                                    value="Trial"/>
                                <Video
                                    jcr:primaryType="nt:unstructured"
                                    text="Video"
                                    value="Video"/>
                                <Webinar
                                    jcr:primaryType="nt:unstructured"
                                    text="Webinar"
                                    value="Webinar"/>
                                <White_x0020_paper
                                    jcr:primaryType="nt:unstructured"
                                    text="White paper"
                                    value="White paper"/>
                            </items>
                        </docTypePrefix>
                        <setChildren
                            granite:class="hide doc-type-prefix-showhide-target foundation-layout-util-vmargin"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                showhidetargetvalue="custom"/>
                            <items jcr:primaryType="nt:unstructured">
                                <heading
                                    granite:class="coral-Heading coral-Heading--4"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/heading"
                                    level="{Long}4"
                                    text="Custom prefix override:"/>
                                <well
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/well">
                                    <items jcr:primaryType="nt:unstructured">
                                        <parentPage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter the custom prefix"
                                            fieldLabel="Custom prefix."
                                            name="./customPrefix"/>
                                    </items>
                                </well>
                            </items>
                        </setChildren>
                        <subtitle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Subtitle"
                            name="./subtitle"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="subtitle"/>
                        </subtitle>
                        <description
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                            fieldLabel="Description"
                            name="./jcr:description"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="jcr:description"/>
                        </description>
                        <short_description
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                            fieldDescription="Used in Related CTAs"
                            fieldLabel="Short Description"
                            name="./short_description"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="short_description"/>
                        </short_description>
                        <meta_description
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                            fieldDescription="Meta Description content is used when the page comes up in search engines."
                            fieldLabel="Meta Description"
                            name="./meta_description"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="meta_description"/>
                        </meta_description>
                        <translation_status
                            cq:showOnCreate="{Boolean}true"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Translation Status"
                            sling:resourceType="granite/ui/components/foundation/form/select"
                            emptyText="Translation Status"
                            fieldDescription="Used to identify the translation status of a page(reports purpose only)."
                            fieldLabel="Translation Status"
                            name="./translation-status">
                            <items jcr:primaryType="nt:unstructured">
                                <Any
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Choose Translation status"
                                    text="Choose Translation status"
                                    value="Choose Translation status"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    text="Translated"
                                    value="Translated"/>
                                <item2
                                    jcr:primaryType="nt:unstructured"
                                    text="Not translated"
                                    value="Not translated"/>
                            </items>
                        </translation_status>
                        <translation_notes
                            jcr:primaryType="nt:unstructured"
                            sling:orderBefore="navigationTitle"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Translation notes(reports purpose only)"
                            fieldLabel="Translation notes"
                            name="./translationNotes"
                            renderReadOnly="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                allowBulkEdit="{Boolean}true"
                                cq-msm-lockable="translation_notes"/>
                        </translation_notes>
                    </items>
                </moretitles>
            </items>
        </column>
    </items>
</jcr:root>
