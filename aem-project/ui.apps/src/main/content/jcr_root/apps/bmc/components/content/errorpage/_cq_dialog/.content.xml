<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="404 Error Page Component Dialog."
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <html
                jcr:primaryType="nt:unstructured"
                jcr:title="Error Page Component"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter the heading text"
                                fieldLabel="Enter the heading text"
                                name="./headerText"/>
                            <subheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter the sub-heading text"
                                fieldLabel="Enter the sub-heading text"
                                name="./subHeaderText"/>
                            <ctatext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Optional: Enter the button text"
                                fieldLabel="Optional: Enter the  button text (if blank, no button rendered)"
                                name="./ctatext"/>
                            <ctapath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Optional: Select the asset this button links to"
                                fieldLabel="Optional: Select the asset the CTA button links to"
                                name="./ctaurl"/>
                            <supportheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter the heading text for support error."
                                fieldLabel="Enter the heading text  for support error."
                                name="./supportheaderText"/>
                            <supportsubheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter the sub-heading text for support central."
                                fieldLabel="Enter the sub-heading text for support central."
                                name="./supportsubHeaderText"/>
                            <supportctatext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Optional: Enter the CTA button text for Support Central "
                                fieldLabel="Optional: Enter the CTA button text (if blank, no button rendered) for Support Central."
                                name="./supportctatext"/>
                            <supportctapath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Optional: Select the Support Central asset this button links to"
                                fieldLabel="Optional: Select the Support Central asset the CTA button links to"
                                name="./supportctaurl"/>
                            <introText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter the intro text"
                                fieldLabel="Enter the intro text"
                                name="./introText"/>
                        </items>
                    </column>
                </items>
            </html>
        </items>
    </content>
</jcr:root>
