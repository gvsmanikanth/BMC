<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="HeaderEducation"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.image.v1.editor]">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <fieldset
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Status Router Config"
                                        sling:orderBefore="title"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <availableCheckbox
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                name="./available"
                                                text="Target Available"
                                                value="true"/>
                                            <availableType
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                name="./available@TypeHint"
                                                value="Boolean"/>
                                            <urlProd
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldDescription="Enter the Production Destination URL for the service router"
                                                fieldLabel="Destination URL Production"
                                                name="./destination_url"/>
                                            <urlStage
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldDescription="Enter the Stage Destination URL for the service router"
                                                fieldLabel="Destination URL Stage"
                                                name="./destination_url_stage"/>
                                            <urlDev
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldDescription="Enter the Dev Destination URL for the service router"
                                                fieldLabel="Destination URL Dev"
                                                name="./destination_url_dev"/>
                                        </items>
                                    </fieldset>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <advanced jcr:primaryType="nt:unstructured"/>
                    <thumbnail jcr:primaryType="nt:unstructured"/>
                    <socialmedia
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <cloudservices jcr:primaryType="nt:unstructured"/>
                    <personalization
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <permissions jcr:primaryType="nt:unstructured"/>
                    <blueprint jcr:primaryType="nt:unstructured"/>
                    <livecopy jcr:primaryType="nt:unstructured"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
