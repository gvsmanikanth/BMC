<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Basic"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <html
                jcr:primaryType="nt:unstructured"
                jcr:title="Header Text"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <headerText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldDescription="Enter the heading text"
                                    fieldLabel="Enter the heading text"
                                    name="./headerText"/>
                            <tab1title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter the text for tab 1"
                                    fieldLabel="Enter the text for tab 1"
                                    name="./tab1title"/>
                            <tab1hash
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional custom hash location"
                                    fieldLabel="Optional custom hash"
                                    name="./tab1hash"/>
                            <tab2title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter the text for tab 2"
                                    fieldLabel="Enter the text for tab 2"
                                    name="./tab2title"/>
                            <tab2hash
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional custom hash location"
                                    fieldLabel="Optional custom hash"
                                    name="./tab2hash"/>
                            <tab3title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter the text for tab 3"
                                    fieldLabel="Enter the text for tab 3"
                                    name="./tab3title"/>
                            <tab3hash
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional custom hash location"
                                    fieldLabel="Optional custom hash"
                                    name="./tab3hash"/>
                            <tab4title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter the text for tab 4"
                                    fieldLabel="Enter the text for tab 4"
                                    name="./tab4title"/>
                            <tab4hash
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional custom hash location"
                                    fieldLabel="Optional custom hash"
                                    name="./tab4hash"/>
                            <tab5title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter the text for tab 5"
                                    fieldLabel="Enter the text for tab 5"
                                    name="./tab5title"/>
                            <tab5hash
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional custom hash location"
                                    fieldLabel="Optional custom hash"
                                    name="./tab5hash"/>
                            <tab6title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter the text for tab 6"
                                    fieldLabel="Enter the text for tab 6"
                                    name="./tab5title"/>
                            <tab6hash
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional custom hash location"
                                    fieldLabel="Optional custom hash"
                                    name="./tab6hash"/>
                        </items>
                    </column>
                </items>
            </html>
        </items>
    </content>
</jcr:root>
