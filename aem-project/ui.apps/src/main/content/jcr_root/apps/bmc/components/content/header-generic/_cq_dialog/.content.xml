<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Generic"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <html
                jcr:primaryType="nt:unstructured"
                jcr:title="Header Text"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <headerText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldDescription="Enter the heading text"
                                    fieldLabel="Enter the heading text"
                                    name="./headerText"/>
                            <descriptionText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldDescription="Enter the description text"
                                    fieldLabel="Enter the description text"
                                    name="./descriptionText"/>

                            <cta1text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional: Enter the 1st CTA link text"
                                    fieldLabel="Optional: Enter the 1st CTA link text"
                                    name="./cta1text"/>
                            <cta1URL
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Enter the 1st CTA link URL"
                                    fieldLabel="Optional: Enter the 1st CTA link URL"
                                    name="./cta1url"/>

                            <cta2text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional: Enter the 2nd button text"
                                    fieldLabel="Optional: Enter the 2nd button text"
                                    name="./cta2text"/>

                            <cta2URL
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional: Enter the 2nd button URL"
                                    fieldLabel="Optional: Enter the 2nd button URL"
                                    name="./cta2url"/>

                            <cta3text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional: Enter the 3rd button text"
                                    fieldLabel="Optional: Enter the 3rd button text"
                                    name="./cta3text"/>

                            <cta3URL
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional: Enter the 3rd button URL"
                                    fieldLabel="Optional: Enter the 3rd button URL"
                                    name="./cta3url"/>

                            <cta4text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional: Enter the 4th button text"
                                    fieldLabel="Optional: Enter the 24thnd button text"
                                    name="./cta4text"/>

                            <cta4URL
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Optional: Enter the 4th button URL"
                                    fieldLabel="Optional: Enter the 4th button URL"
                                    name="./cta4url"/>
                        </items>
                    </column>
                </items>
            </html>
        </items>
    </content>
</jcr:root>
