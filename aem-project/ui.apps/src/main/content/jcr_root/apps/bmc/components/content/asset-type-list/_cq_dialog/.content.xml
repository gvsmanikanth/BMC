                                                text="none selected"
                                                value=""/>
                                            <icon-document
                                                jcr:primaryType="nt:unstructured"
                                                text="Document icon"
                                                value="icon-document"/>
                                            <icon-documents
                                                jcr:primaryType="nt:unstructured"
                                                text="Documents icon"
                                                value="icon-documents"/>
                                            <resource-video
                                                jcr:primaryType="nt:unstructured"
                                                text="Video icon"
                                                value="resource-video"/>
                                        </items>
                                    </iconLink2>
                                    <assetsSubheadingPrefix2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Select asset 2 sub heading prefix. (:) will be appended automatically"
                                        multiple="{Boolean}false"
                                        name="./assetsSubheadingPrefix2">
                                        <items jcr:primaryType="nt:unstructured">
                                            <noneselected
                                                jcr:primaryType="nt:unstructured"
                                                text="none selected"
                                                value=""/>
                                            <datasheet
                                                jcr:primaryType="nt:unstructured"
                                                text="Datasheet"
                                                value="datasheet"/>
                                            <datasheets
                                                jcr:primaryType="nt:unstructured"
                                                text="Datasheets"
                                                value="datasheets"/>
                                        </items>
                                    </assetsSubheadingPrefix2>
                                    <assetsSubheading2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Use sentence case."
                                        fieldLabel="Asset Sub Heading 2"
                                        name="./assetsSubheading2"/>
                                    <assetLinks2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Asset Links 2">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            name="./assetLinks2"
                                            wrapperClass="core-wcm-form-options-toggle-set">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/layouts/fixedcolumns"
                                                method="absolute"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <overideTitle
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Override Title"
                                                            fieldLabel="Title Override (Optional)"
                                                            name="./overrideTitle"/>
                                                        <pagePath
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Use the path picker to select a page within the site or enter a URL beginning with http:"
                                                            fieldLabel="Page Path or external URL"
                                                            name="./pagePath"
                                                            rootPath="/content/bmc"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </assetLinks2>
                                </items>
                            </column>
                        </items>
                    </assetLinks2Set>
                    <assetLinks3Set
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Asset Links 3"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <iconLink3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Select asset 3 icon"
                                        multiple="{Boolean}false"
                                        name="./iconLink3">
                                        <items jcr:primaryType="nt:unstructured">
                                            <noneselected
                                                jcr:primaryType="nt:unstructured"
                                                text="none selected"
                                                value=""/>
                                            <icon-document
                                                jcr:primaryType="nt:unstructured"
                                                text="Document icon"
                                                value="icon-document"/>
                                            <icon-documents
                                                jcr:primaryType="nt:unstructured"
                                                text="Documents icon"
                                                value="icon-documents"/>
                                            <resource-video
                                                jcr:primaryType="nt:unstructured"
                                                text="Video icon"
                                                value="resource-video"/>
                                        </items>
                                    </iconLink3>
                                    <assetsSubheadingPrefix3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Select asset 3 sub heading prefix. (:) will be appended automatically"
                                        multiple="{Boolean}false"
                                        name="./assetsSubheadingPrefix3">
                                        <items jcr:primaryType="nt:unstructured">
                                            <noneselected
                                                jcr:primaryType="nt:unstructured"
                                                text="none selected"
                                                value=""/>
                                            <datasheet
                                                jcr:primaryType="nt:unstructured"
                                                text="Datasheet"
                                                value="datasheet"/>
                                            <datasheets
                                                jcr:primaryType="nt:unstructured"
                                                text="Datasheets"
                                                value="datasheets"/>
                                        </items>
                                    </assetsSubheadingPrefix3>
                                    <assetSubheading3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Use sentence case."
                                        fieldLabel="Asset Sub Heading 3"
                                        name="./assetsSubheading3"/>
                                    <assetLinks3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Asset Links 3">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            name="./assetLinks3"
