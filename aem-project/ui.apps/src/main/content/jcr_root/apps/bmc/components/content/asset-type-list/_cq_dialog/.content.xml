                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/layouts/fixedcolumns"
                                                method="absolute"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <overideTitle
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Override Title"
                                                            fieldLabel="Title Override (Optional)"
                                                            name="./overrideTitle"/>
                                                        <pagePath
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Use the path picker to select a page within the site or enter a URL beginning with http:"
                                                            fieldLabel="Page Path or external URL"
                                                            name="./pagePath"
                                                            rootPath="/content/bmc"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </assetLinks3>
                                </items>
                            </column>
                        </items>
                    </assetLinks3Set>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
