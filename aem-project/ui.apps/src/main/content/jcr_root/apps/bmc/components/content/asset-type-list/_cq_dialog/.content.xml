<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Asset Type and List Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <assetTypeList
                jcr:primaryType="nt:unstructured"
                jcr:title="Asset Type and List"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <iconLink1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Select asset 1 icon"
                                multiple="{Boolean}false"
                                name="./iconLink1">
                                <items jcr:primaryType="nt:unstructured">
                                    <noneselected
                                        jcr:primaryType="nt:unstructured"
                                        text="none selected"
                                        value=""/>
                                    <icon-document
                                        jcr:primaryType="nt:unstructured"
                                        text="Document icon"
                                        value="icon-document"/>
                                    <icon-documents
                                        jcr:primaryType="nt:unstructured"
                                        text="Documents icon"
                                        value="icon-documents"/>
                                    <resource-video
                                        jcr:primaryType="nt:unstructured"
                                        text="Video icon"
                                        value="resource-video"/>
                                </items>
                            </iconLink1>
                            <assetsSubheadColor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Select Subheading color .. use case industry-healthcare.html"
                                multiple="{Boolean}false"
                                name="./assetsSubheadColor">
                                <items jcr:primaryType="nt:unstructured">
                                    <noneselected
                                        jcr:primaryType="nt:unstructured"
                                        text="none selected"
                                        value=""/>
                                    <orange
                                        jcr:primaryType="nt:unstructured"
                                        text="Orange"
                                        value="Orange"/>
                                </items>
                            </assetsSubheadColor>
                            <assetsSubheadingPrefix1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Select asset 1 sub heading prefix. (:) will be appended automatically"
                                multiple="{Boolean}false"
                                name="./assetsSubheadingPrefix1">
                                <items jcr:primaryType="nt:unstructured">
                                    <noneselected
                                        jcr:primaryType="nt:unstructured"
                                        text="none selected"
                                        value=""/>
                                    <datasheet
                                        jcr:primaryType="nt:unstructured"
                                        text="Datasheet"
                                        value="datasheet"/>
                                    <datasheets
                                        jcr:primaryType="nt:unstructured"
                                        text="Datasheets"
                                        value="datasheets"/>
                                </items>
                            </assetsSubheadingPrefix1>
                            <assetSubheading1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Use sentence case."
                                fieldLabel="Asset Sub Heading 1"
                                name="./assetSubheading1"/>
                            <assetLinks1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Asset Links 1">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                    name="./assetLinks1"
                                    wrapperClass="core-wcm-form-options-toggle-set">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <overideTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Override Title"
                                                    fieldLabel="Title Override (Optional)"
                                                    name="./overrideTitle"/>
                                                <pagePath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldDescription="Use the path picker to select a page within the site or enter a URL beginning with http:"
                                                    fieldLabel="Page Path or external URL"
                                                    name="./pagePath"
                                                    rootPath="/content/bmc"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </assetLinks1>
                        </items>
                    </column>
                </items>
            </assetTypeList>
        </items>
    </content>
</jcr:root>
