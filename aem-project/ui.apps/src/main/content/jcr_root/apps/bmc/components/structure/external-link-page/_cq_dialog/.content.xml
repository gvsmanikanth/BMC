<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="HeaderEducation"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.image.v1.editor]">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <config
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="External Link "
                                        sling:orderBefore="title"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <linkAbstractorExternalURL
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldDescription="Enter the link URL."
                                                fieldLabel="Link URL "
                                                name="./linkAbstractorExternalURL"
                                                text="Link URL"/>
                                            <linkAbstractor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                defaultValue="external-link"
                                                name="./linkAbstractor"
                                                value="external-link"/>
                                            <linkAbstractorTarget
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                fieldDescription="Enter the link target."
                                                fieldLabel="Link Target"
                                                name="./linkAbstractorTarget">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <item1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Load in a new window (_blank)"
                                                        value="new"/>
                                                    <item3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Load in the same window (_self)"
                                                        value="self"/>
                                                </items>
                                            </linkAbstractorTarget>
                                        </items>
                                    </config>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <advanced jcr:primaryType="nt:unstructured"/>
                    <thumbnail jcr:primaryType="nt:unstructured"/>
                    <socialmedia
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <cloudservices jcr:primaryType="nt:unstructured"/>
                    <personalization
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <permissions jcr:primaryType="nt:unstructured"/>
                    <blueprint jcr:primaryType="nt:unstructured"/>
                    <livecopy jcr:primaryType="nt:unstructured"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
